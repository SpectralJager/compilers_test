// import statement
@import "http" as http;

// global varible statement
@var alpha:int = 20;
@var beta:string = "some string";

// global constant statement
@const hello = "hello world";

// define data type
@def myInt int;
@def myStruct $(
    field1:int
    field2:string
);

// function
@fn func:void[a:myInt b:myStruct] {
    @var radius:int = 10;
    @var result:float = (itof (iadd radius alpha));
    @return;
}