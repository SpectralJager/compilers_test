(@module :main
    (@const n 10)

    (@fn fib [n::int] <int> :do
        (@if (int/lt n 2) :then
            (@return n) 
        ) 
        (@return (int/add
            (fib (int/sub n 1))
            (fib (int/sub n 2))))
    )

    (@fn main :do
        (@var res :of int (fib n))
        (io/println "fib of" (int/toString n) "equal" (int/toString res)) 
        (exit 0)
    )
)