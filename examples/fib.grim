(@module :main

    (@fn fib [n::int] <int> :do
        (@if (int/lt n 2) :then
            (@return n) 
        ) 
        (@return (int/add 
            (fib (int/sub n 1))
            (fib (int/sub n 2))))
    )

    (@fn main :do
        (@const n 30)
        (io/println (string/format "fib($) = $" 
            (int/toString n) 
            (int/toString (fib n)))
        )
        (exit 0)
    )
)