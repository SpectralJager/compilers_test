init|>
	new.const n 40;
	call main;
	hlt;
code|>
fib:
    new.var n int;
    pop.var n;
    push.var n;
    push.val 2;
    reg.val ian 2;
    call.sys int.lt;
    jump.cond if_end;
    push.var n;
    ret;
    :if_end
    push.var n;
    push.val 1;
    reg.val ian 2;
    call.sys int.sub;
    push.var n;
    push.val 2;
    reg.val ian 2;
    call.sys int.sub;
    reg.val ian 2;
    call.sys int.add;
    call fib;
    ret
main:
    new.var result int;
    push.const n;
    call fib;
    pop.var result;
    push.val "result: %d\n";
    push.var result;
    reg.val ian 2;
    call.sys printf;
    ret;